<script>
    const form = document.querySelector('#addqform');

form.addEventListener('submit', (e) => {
  e.preventDefault(); // prevent form submission

  const formData = new FormData(form); // create a new form data object with the form element

  // send the form data to the server using an AJAX request
  fetch('/uploadq', {
    method: 'POST',
    body: formData
  })
  .then(response => {
    console.log('Form data uploaded successfully!');
    window.location.href = "/admin/addq?u=s"
  })
  .catch(error => {
    console.error('Error uploading form data:', error);
    window.location.href = "/admin/addq?u=e"
  });

});
</script>